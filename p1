Program 1A

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
namespace lab1
{
 internal class Program
 {
 static void Main(string[] args)
 {
 int number = Convert.ToInt32(args[0]);
 double sqrtnum= Math.Sqrt(number);
 sqrtnum = Math.Round(sqrtnum, 2);
 Console.WriteLine("square root of {0} is :{1}",number, sqrtnum);
 Console.ReadLine();
 }
 }
}



Program 1B

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
namespace lab1
{
 internal class Program
 {
 static void Main(string[] args)
 {
 int a, b, c, sum;
 float avg;
 a = int.Parse(args[0]);
 b = int.Parse(args[1]);
 c = int.Parse(args[2]);
 sum=a+b+c;
 Console.WriteLine("sum of three numbers is :"+sum);
 avg=sum/3;
 Console.WriteLine("avrage of three numbers is :"+avg);
 Console.ReadLine(); 
 }
 }
}
